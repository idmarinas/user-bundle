	"autoload": {
		"psr-4": {
			"Idm\\Bundle\\User\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Idm\\Bundle\\User\\Tests\\": "tests/"
		}
	},
	"funding": [
		{
