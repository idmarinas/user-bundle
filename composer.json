		"symfony/phpunit-bridge": "^6.0",
		"symfony/uid": "^5.4 | ^6.4 | ^7.0"
	},
	"conflict": {
		"idmarinas/common-bundle": "3.0.0"
	},
	"config": {
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"symfony/flex": true,
			"symfony/runtime": true
		},
		"optimize-autoloader": false,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
